var simplify = function(a,b){
	var numer = numer || a;
	var denom = denom || b;
	for(i=1;i<=a;i++){
		if (a % i === 0 && b%i===0 ){
			numer = (a/i);
			denom= (b/i);
		}
		else{}
		
	}
	console.log(numer +" over "+ denom);
	
};


simplify(prompt("What is the numerator"),prompt("What is the denominator"));
